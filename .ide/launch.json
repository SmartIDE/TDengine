{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) taosd",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/build/bin/taosd",
            "args": [
                "-c",
                "${workspaceFolder}/sim/cfg"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            // "preLaunchTask": "C/C++: g++ build active file",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "(gdb) taos",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/build/bin/taos",
            "args": [
                "-c",
                "${workspaceFolder}/sim/cfg"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            // "preLaunchTask": "C/C++: g++ build active file",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}